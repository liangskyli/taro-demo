{
  "name": "taro-demo",
  "version": "1.0.0",
  "private": true,
  "description": "taro-demo",
  "templateInfo": {
    "name": "taro-hooks",
    "typescript": true,
    "css": "less"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write",
      "eslint --fix"
    ],
    "*.(css|less)": [
      "prettier --write",
      "stylelint --fix"
    ]
  },
  "scripts": {
    "prepare": "husky",
    "build:weapp": "taro build --type weapp",
    "build:swan": "taro build --type swan",
    "build:alipay": "taro build --type alipay",
    "build:tt": "taro build --type tt",
    "build:h5": "taro build --type h5",
    "build:rn": "taro build --type rn",
    "build:qq": "taro build --type qq",
    "build:jd": "taro build --type jd",
    "build:quickapp": "taro build --type quickapp",
    "dev:weapp": "npm run build:weapp -- --watch",
    "dev:swan": "npm run build:swan -- --watch",
    "dev:alipay": "npm run build:alipay -- --watch",
    "dev:tt": "npm run build:tt -- --watch",
    "dev:h5": "npm run build:h5 -- --watch",
    "dev:rn": "npm run build:rn -- --watch",
    "dev:qq": "npm run build:qq -- --watch",
    "dev:jd": "npm run build:jd -- --watch",
    "dev:quickapp": "npm run build:quickapp -- --watch",
    "coverage": "vitest run --coverage",
    "test": "vitest run",
    "test-u": "vitest -u run",
    "update:deps": "pnpm update --interactive --latest"
  },
  "browserslist": [
    "last 3 versions",
    "Android >= 4.1",
    "ios >= 8"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.28.4",
    "@liangskyli/taro-request": "^0.2.2",
    "@nutui/nutui-react-taro": "^3.0.18",
    "@reduxjs/toolkit": "^2.11.2",
    "@taro-hooks/plugin-react": "^2.2.0",
    "@tarojs/components": "4.1.9",
    "@tarojs/helper": "4.1.9",
    "@tarojs/plugin-framework-react": "4.1.9",
    "@tarojs/plugin-html": "4.1.9",
    "@tarojs/plugin-platform-alipay": "4.1.9",
    "@tarojs/plugin-platform-h5": "4.1.9",
    "@tarojs/plugin-platform-jd": "4.1.9",
    "@tarojs/plugin-platform-qq": "4.1.9",
    "@tarojs/plugin-platform-swan": "4.1.9",
    "@tarojs/plugin-platform-tt": "4.1.9",
    "@tarojs/plugin-platform-weapp": "4.1.9",
    "@tarojs/react": "4.1.9",
    "@tarojs/runtime": "4.1.9",
    "@tarojs/shared": "4.1.9",
    "@tarojs/taro": "4.1.9",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-redux": "^9.2.0",
    "taro-hooks": "^2.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/preset-react": "^7.28.5",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@liangskyli/mock": "^6.0.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.6.2",
    "@tarojs/cli": "4.1.9",
    "@tarojs/service": "4.1.9",
    "@tarojs/taro-loader": "4.1.9",
    "@tarojs/webpack5-runner": "4.1.9",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@types/node": "^22.15.29",
    "@types/react": "^18.3.20",
    "@types/webpack-env": "^1.18.8",
    "@typescript-eslint/eslint-plugin": "^8.50.0",
    "@typescript-eslint/parser": "^8.50.0",
    "@vitest/coverage-v8": "^3.2.4",
    "babel-plugin-import": "^1.13.8",
    "babel-preset-taro": "4.1.9",
    "eslint": "^8.57.1",
    "eslint-config-taro": "4.1.9",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "less": "^4.5.1",
    "lint-staged": "^15.5.1",
    "postcss": "^8.5.6",
    "prettier": "^3.7.4",
    "prettier-plugin-organize-imports": "^4.3.0",
    "prettier-plugin-packagejson": "^2.5.20",
    "react-refresh": "^0.18.0",
    "stylelint": "16.26.1",
    "stylelint-config-recommended-less": "^3.0.1",
    "stylelint-config-standard": "^39.0.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths-webpack-plugin": "^4.2.0",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4",
    "webpack": "^5.104.1"
  },
  "packageManager": "pnpm@10.11.0",
  "engines": {
    "node": ">=18.20.0",
    "pnpm": "^10"
  }
}
